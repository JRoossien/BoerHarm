package nl.josroossien.edugame.objects {import flash.display.Sprite;public class Block extends Sprite {		public static var block:Block;		public function Block(x, y, width, heigth, color:uint=0x00AA00) {		graphics.beginFill(color);		graphics.drawRect(0,0,width,heigth);		graphics.endFill();		this.x = x;		this.y = y;	}		public static function getBlock():Block {		return block;	}}}